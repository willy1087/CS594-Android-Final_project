    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/startDate_row"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/amount"
            android:textColor="@color/input_text_color"
            android:textSize="25sp"
            android:layout_weight="0.5"
            />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:textColor="@color/text_color"
            android:id="@+id/amount"
            android:hint="@string/startDate_hint"
            android:layout_weight="0.5"
            />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/endDate_row"
        android:layout_below="@+id/startDate_row"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/endDate"
            android:textColor="@color/input_text_color"
            android:textSize="25sp"
            android:layout_weight="0.5"
            />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:textColor="@color/text_color"
            android:id="@+id/endDate"
            android:hint="@string/endDate_hint"
            android:layout_weight="0.5"
            />

    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/endDate_row"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="@string/type"
            android:textColor="@color/input_text_color"
            android:textSize="25sp"
            android:layout_weight="0.5"
            />

        <Spinner
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:id="@+id/type"
            android:background="#000000"
            android:layout_weight="0.5"
            android:entries="@array/report_values"
            android:paddingTop="15dp"
            android:paddingBottom="15dp">

        </Spinner>

    </LinearLayout>


